  "components": {
    "schemas": {
      "CarListing": {
        "required": [
          "brand",
          "carType",
          "engineCapacity",
          "fuelType",
          "rentalPricePerDay",
          "seats"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "brand": {
            "minLength": 1,
            "type": "string"
          },
          "engineCapacity": {
            "type": "number",
            "format": "double"
          },
          "fuelType": {
            "minLength": 1,
            "type": "string"
          },
          "seats": {
            "type": "integer",
            "format": "int32"
          },
          "carType": {
            "minLength": 1,
            "type": "string"
          },
          "features": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "latitude": {
            "type": "number",
            "format": "double"
          },
          "longitude": {
            "type": "number",
            "format": "double"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "isAvailable": {
            "type": "boolean"
          },
          "isApproved": {
            "type": "boolean"
          },
          "rentalPricePerDay": {
            "type": "number",
            "format": "double"
          },
          "averageRating": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CarRentalRequest": {
        "type": "object",
        "properties": {
          "carListingId": {
            "type": "integer",
            "format": "int32"
          },
          "rentalStartDate": {
            "type": "string",
            "format": "date-time"
          },
          "rentalEndDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "CarRentalReviewRequest": {
        "type": "object",
        "properties": {
          "carRentalId": {
            "type": "integer",
            "format": "int32"
          },
          "rating": {
            "type": "integer",
            "format": "int32"
          },
          "comment": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ChangeUsernameModel": {
        "required": [
          "newUsername"
        ],
        "type": "object",
        "properties": {
          "newUsername": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "LoginModel": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RegisterModel": {
        "required": [
          "confirmPassword",
          "email",
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ResetPasswordModel": {
        "required": [
