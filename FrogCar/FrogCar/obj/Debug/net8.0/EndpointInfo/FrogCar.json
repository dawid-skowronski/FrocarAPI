          },
          "seats": {
            "type": "integer",
            "format": "int32"
          },
          "carType": {
            "minLength": 1,
            "type": "string"
          },
          "features": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "latitude": {
            "type": "number",
            "format": "double"
          },
          "longitude": {
            "type": "number",
            "format": "double"
          },
          "userId": {
            "type": "integer",
